{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~api/*": [
                "src/api/*"
            ],
            "~common/*": [
                "src/common/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~pages/*": [
                "src/pages/*"
            ],
            "~router/*": [
                "src/router/*"
            ],
            "~/*": [
                "src/*"
            ]
        }
    }
}